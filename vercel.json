{
  "version": 2,
  "cleanUrls": true,
  "headers": [
    {
      "source": "/(.*).js",
      "headers": [
        { "key": "Content-Type", "value": "application/javascript" },
        { "key": "Cache-Control", "value": "no-store, must-revalidate" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/weather-bulletin", "destination": "/engine/weather-bulletin.js" },
    { "source": "/engine/weather-engine", "destination": "/engine/weather-engine.js" },
    { "source": "/velocity-widget", "destination": "/velocity-widget.js" },
    { "source": "/engine/route-engine", "destination": "/engine/route-engine.js" }
  ]
}
